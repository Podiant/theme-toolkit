<!-- List of subscription links (Apple Podcasts, Pocket Casts, Overcast, etc) -->
<ul>
  {{# each subscription_links }}
    <li class="subscribe-{{ link_class }}">
      <a href="{{ link_url }}" target="_blank">
        <img src="{{ link_icon }}" />
        {{ link_title }}
      </a>
    </li>
  {{/ each }}
</ul>
